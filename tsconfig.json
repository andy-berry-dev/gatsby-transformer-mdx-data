{
    "compilerOptions": {
        "target": "ES2016",
        "rootDir": ".",
        "outDir": "dist",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "declaration": true
    },
    "include": [
        "./index.ts",
        "./src/**/*",
        "./gatsby-*.ts",
        "./gatsby-*.tsx",
    ],
}

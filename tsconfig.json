{
    "compilerOptions": {
        "target": "ES2016",
        "rootDir": ".",
        "outDir": "dist",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "declaration": true,
        "jsx": "react"
    },
    "include": ["./index.ts", "./*.tsx", "./gatsby-*.ts", "./gatsby-*.tsx"]
}
